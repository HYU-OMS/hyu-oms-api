{
  "name": "hyu-oms-api",
  "version": "1.0.0",
  "description": "HYU-OMS API",
  "scripts": {
    "start-dev": "DEBUG=hyu-oms-api:* NODE_ENV=development babel-node src/scripts/run.js",
    "start": "npm run build && cd build && NODE_ENV=production node scripts/run.js",
    "build": "NODE_ENV=production babel src -d build && cp -rf node_modules build/node_modules"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.16.4",
    "express-asyncify": "^1.0.0",
    "helmet": "^3.15.0",
    "http-errors": "^1.7.1",
    "jsonwebtoken": "^8.4.0",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.4",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "uuid": "^3.3.2",
    "xlsx": "^0.14.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.3"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "targets": {
            "node": "10"
          }
        }
      ]
    ]
  },
  "author": "Kihoon Han <kordreamfollower@gmail.com>",
  "license": "MIT"
}
